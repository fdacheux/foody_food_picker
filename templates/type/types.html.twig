{% extends 'base.html.twig' %}

{% block title %}Types of food{% endblock %}

{% block myTitle %}List of all categories of food :{% endblock %}

{% block body %}

<div class="row ms-1">
{% for type in types %}
    <div class="col-6">
        <div class="row align-items-center">
            <div class="col-6 col-md-4col-lg-2">
                <img  class="perso_imgFood img-fluid" src="{{asset('images/type/' ~ type.image)}}" alt="">
           </div>
            <div class="col">
                {{ type.fieldName }}
            </div>
        </div>
        
    </div>
{% endfor %}
</div>
{% endblock %}
